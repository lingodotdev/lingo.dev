{
  "name": "lingo.dev",
  "version": "0.70.1",
  "description": "Lingo.dev",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./cli": {
      "types": "./build/cli.d.ts",
      "import": "./build/cli.mjs",
      "require": "./build/cli.cjs"
    },
    "./sdk": {
      "types": "./build/sdk.d.ts",
      "import": "./build/sdk.mjs",
      "require": "./build/sdk.cjs"
    },
    "./spec": {
      "types": "./build/spec.d.ts",
      "import": "./build/spec.mjs",
      "require": "./build/spec.cjs"
    }
  },
  "bin": {
    "lingo.dev": "./bin/cli.mjs"
  },
  "files": [
    "bin",
    "build"
  ],
  "scripts": {
    "lingo.dev": "node --inspect ./bin/cli.mjs",
    "clean": "rm -rf build",
    "build": "tsup"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@lingo.dev/cli": "workspace:*",
    "@lingo.dev/sdk": "workspace:*",
    "@lingo.dev/spec": "workspace:*",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "tsup": "^8.3.5"
  }
}
