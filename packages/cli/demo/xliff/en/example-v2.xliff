<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:2.0"
       version="2.0"
       srcLang="en"
       trgLang="fr">

  <file id="messages.properties" original="messages.properties">

    <notes>
      <note>This header content is not localized</note>
    </notes>


    <unit id="welcome_message">
      <segment>
        <source>Hello, world!</source>

        <target state="translated">Bonjour le monde!</target>
      </segment>
    </unit>

    <unit id="error_message" type="resname">
      <segment>
        <source><![CDATA[Enter valid <email>]]></source>
        <target state="new"><![CDATA[Entrez <email> valide]]></target>
      </segment>
    </unit>

    <unit id="button_text">
      <segment>
        <source>Continue</source>
        <target state="translated">Continuer</target>
      </segment>
    </unit>
  </file>
</xliff>
