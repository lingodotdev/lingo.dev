<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:2.0"
       version="2.0"
       srcLang="en"
       trgLang="fr">

  <file id="messages.properties" original="messages.properties">

    <notes>
      <note>This header content is not localized</note>
    </notes>


    <unit id="welcome_message">
      <segment>
        <source>Welcome to our application</source>

        <target state="translated">Bienvenue dans notre application</target>
      </segment>
    </unit>

    <unit id="error_message" type="resname">
      <segment>
        <source><![CDATA[Please enter a valid <email> address]]></source>
        <target state="new"><![CDATA[Veuillez saisir une adresse <email> valide]]></target>
      </segment>
    </unit>

    <unit id="button_text">
      <segment>
        <source>Click here to continue</source>
        <target state="translated">Cliquez ici pour continuer</target>
      </segment>
    </unit>
  </file>
</xliff>
