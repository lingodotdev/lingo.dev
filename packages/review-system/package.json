{
  "name": "@lingo.dev/review-system",
  "version": "0.1.0",
  "description": "Multi-Agent Translation Review System for Lingo.dev",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@lingo.dev/sdk": "workspace:*",
    "@lingo.dev/_spec": "workspace:*",
    "zod": "^3.22.4",
    "@paralleldrive/cuid2": "^2.2.2",
    "openai": "^4.20.1",
    "anthropic": "^0.7.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@lingo.dev/sdk": "workspace:*"
  },
  "keywords": [
    "lingo.dev",
    "translation",
    "review",
    "multi-agent",
    "ai",
    "localization",
    "i18n"
  ],
  "author": "Lingo.dev Team",
  "license": "MIT"
} 