{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "typecheck",
        "^build"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "prepare:e2e": {
      "dependsOn": [
        "^build"
      ]
    },
    "test:e2e": {
      "dependsOn": [
        "prepare:e2e"
      ]
    },
    "deploy": {
      "dependsOn": [
        "build",
        "test",
        "^deploy"
      ]
    }
  }
}
